{
    "name": "clitable",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --no-stdin --watch src --ext ts,json --exec \"npm run dev:build\"",
        "dev:build": "node --inspect --require ts-node/register src/index.ts",
        "dev:brk": "nodemon --watch src/** --ext ts,json --exec \"npm run dev:build:brk\"",
        "dev:build:brk": "node --inspect-brk --require ts-node/register src/index.ts",
        "start": "node dist/index.js",
        "build": "tsc",
        "build:cpp": "node-gyp rebuild"
    },
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "ansi-escapes": "^5.0.0",
        "node-addon-api": "^4.2.0",
        "nodemon": "^2.0.15",
        "sisteransi": "^1.0.5",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.2"
    }
}
